# Backend'i 9090'da dinlet (frontend kodu 9090 bekliyor)
APP_PORT=9090

# DB: VM'deki container'a bağlan
DATABASE_URL=postgres://postgres:postgres@192.168.248.133:5432/go_nmos?sslmode=disable

JWT_SECRET=murat

# Frontend'i local dev'den açıyorsun -> Vite default: 5173
